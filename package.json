    "prettier": "^3.5.2",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^19.0.0",
    "rimraf": "^5.0.1",
    "sass": "^1.85.1",
    "sass-loader": "^16.0.5",
    "storybook": "^8.6.3",
